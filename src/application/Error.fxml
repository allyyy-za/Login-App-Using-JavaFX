<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.ErrorController">
   <children>
      <Label fx:id="laa" layoutX="52.0" layoutY="18.0" prefHeight="54.0" prefWidth="324.0" text="ERROR IN LOGIN" underline="true">
         <font>
            <Font name="System Bold Italic" size="36.0" />
         </font></Label>
      <Label layoutX="11.0" layoutY="63.0" prefHeight="71.0" prefWidth="389.0" text="* Current Username and password to locate your account">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <TextField fx:id="txterroruser" layoutX="89.0" layoutY="139.0" promptText="Username">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <PasswordField fx:id="txterrorpass" layoutX="89.0" layoutY="201.0" promptText="Password">
         <font>
            <Font size="18.0" />
         </font>
      </PasswordField>
      <Button layoutX="92.0" layoutY="262.0" mnemonicParsing="false" onAction="#submit" prefHeight="39.0" prefWidth="76.0" text="Submit" />
      <Button layoutX="237.0" layoutY="264.0" mnemonicParsing="false" onAction="#exxit" prefHeight="39.0" prefWidth="76.0" text="EXIT" />
      <Button layoutX="102.0" layoutY="361.0" mnemonicParsing="false" onAction="#out" prefHeight="26.0" prefWidth="183.0" text="Try another account Login" />
      <Label fx:id="mymessage" layoutX="45.0" layoutY="318.0" prefHeight="18.0" prefWidth="338.0">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
